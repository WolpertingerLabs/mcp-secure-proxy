{
  "name": "GitHub API",
  "description": "GitHub REST API â€” repositories, issues, pull requests, users, organizations, and more. Auth is handled automatically via the GITHUB_TOKEN environment variable.",
  "docsUrl": "https://docs.github.com/en/rest",
  "openApiUrl": "https://raw.githubusercontent.com/github/rest-api-description/main/descriptions/api.github.com/api.github.com.json",
  "headers": {
    "Authorization": "Bearer ${GITHUB_TOKEN}",
    "Accept": "application/vnd.github+json",
    "X-GitHub-Api-Version": "2022-11-28",
    "User-Agent": "mcp-secure-proxy"
  },
  "secrets": {
    "GITHUB_TOKEN": "${GITHUB_TOKEN}"
  },
  "allowedEndpoints": [
    "https://api.github.com/**"
  ]
}
